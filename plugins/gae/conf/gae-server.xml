<?xml version="1.0" encoding="utf-8"?>

<Configure>
	<Compress-Filter>
		<content-type>audio</content-type>
		<content-type>video</content-type>
		<content-type>image</content-type>
		<content-type>/zip</content-type>
		<content-type>/x-gzip</content-type>
		<content-type>/x-zip-compressed</content-type>
		<content-type>/x-compress</content-type>
		<content-type>/x-compressed</content-type>
		<content-type>/x-spoon</content-type>
	</Compress-Filter>

	<MaxXMPPDataPackageSize>40960</MaxXMPPDataPackageSize>
	<FetchRetryCount>1</FetchRetryCount>
	<RangeFetchLimit>512000</RangeFetchLimit>
	<Compressor>Snappy</Compressor>
	<Encrypter>SE1</Encrypter>
	<IsMaster>false</IsMaster>
</Configure>